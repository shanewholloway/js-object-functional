{
  "name": "object-functional",
  "version": "0.4.1",
  "description": "Object-Functional programming paradigm for JavaScript, including change isolation and frozen views.",
  "license": "BSD-2-Clause",
  "author": "Shane Holloway <shane.holloway@ieee.org>",
  "homepage": "https://github.com/shanewholloway/js-object-functional#readme",
  "bugs": {
    "url": "https://github.com/shanewholloway/js-object-functional/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/shanewholloway/js-object-functional.git"
  },
  "keywords": [
    "object-functional",
    "object",
    "functional",
    "redux",
    "reducer",
    "action",
    "notification",
    "immutable",
    "view"
  ],
  "files": [
    "dist/",
    "code/",
    "test/",
    "*.js"
  ],
  "main": "dist/index.js",
  "browser": "dist/index.min.js",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-jsy": "^0.7.2",
    "babili": "^0.1.1",
    "deep-freeze": "^0.0.1",
    "immu": "^3.0.0",
    "immutable": "^3.8.1",
    "seamless-immutable": "^7.1.2",
    "tap-lite-tester": "^0.4.0"
  },
  "babel": {
    "presets": [
      [
        "jsy",
        {
          "targets": {
            "node": "7.1",
            "browsers": "> 5%"
          }
        }
      ]
    ]
  },
  "scripts": {
    "start": "node dist",
    "build": "npm -s run build:code && npm -s run build:min",
    "build:code": "babel -s true -x .js,.jsy code -d dist",
    "build:min": "babili --no-babelrc -s true dist/index.js -o dist/index.min.js",
    "watch": "npm -s run build:code -- --watch",
    "prepublish": "npm -s run test",
    "pretest": "npm -s run build",
    "test": "npm -s run :test",
    ":test": "babel-node ./test/test.jsy",
    "shellenv": "export PATH=$PWD/node_modules/.bin:$PATH; cd examples; echo 'Run babel-node ./demo-simple-counter.jsy'; $SHELL",
    "example": "babel-node ./example/demo.jsy"
  }
}
